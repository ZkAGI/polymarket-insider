  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "@prisma/client": "^6.19.1",
    "dotenv": "^17.2.3",
    "grammy": "^1.39.2",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.7.0",
    "swr": "^2.3.8",
    "viem": "^2.44.1"
