{
  "name": "polymarket-tracker",
  "version": "1.0.0",
  "description": "Polymarket Insider/Whale Tracker - Track and analyze large trades and insider activity on Polymarket prediction markets",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "polymarket",
    "prediction-markets",
    "whale-tracking",
    "crypto",
    "analytics"
